                                        "type": "string",
                                        "example": "Soltero"
                                    },
                                    "description": {
                                        "title": "Descripción",
                                        "description": "Descripción del Escritor.",
                                        "type": "string",
                                        "nullable": true
                                    },
                                    "favorite_gender": {
                                        "title": "Género Favorito",
                                        "description": "Género Favorito del Lector.",
                                        "type": "string",
                                        "example": "Terror"
                                    },
                                    "reading_hours": {
                                        "title": "Horas de Lecturas",
                                        "description": "Horas de Lecturas del Lector.",
                                        "type": "integer",
