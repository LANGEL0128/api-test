    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Inicia la sesión",
                "description": "Inicia la sesión del usuario",
                "operationId": "Iniciar Sesión",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "schema": "Request",
                                "title": "Title",
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "title": "Correo",
                                        "description": "El correo del usuario a loguear",
                                        "type": "string",
                                        "example": "ddf@gmail.com"
                                    },
                                    "password": {
                                        "title": "Contraseña",
                                        "description": "La contraseña del usuario a loguear",
                                        "type": "string",
                                        "example": "123456"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "email": "ddf@gmail.com",
                                    "password": "123456"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "access_token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIiwibmFtZSI6IkpvaG4gRG9lIiwiYWRtaW4iOnRydWV9.TJVA95OrM7E2cBab30RMHrHDcEfxjoYZgeFONFh7HgQ",
                                    "token_type": "bearer",
                                    "expires_in": 3600
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unahutorized",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "message": "Unahutorized"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/auth/me": {
            "get": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Mostrar Datos del Usaurio",
                "description": "Mostrar Datos del Usaurio logueado",
                "operationId": "Mostrar Datos del Usaurio",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "message": "Ha cerrado sesión correctamente"
                                }
                            }
                        }
